    "@nuxtjs/color-mode": "^3.3.0",
    "@pinia/nuxt": "^0.4.11",
    "@sidebase/nuxt-auth": "0.6.0-beta.5",
    "@unocss/eslint-config": "^0.55.4",
    "@unocss/nuxt": "^0.55.4",
    "@vite-pwa/nuxt": "^0.1.0",
    "@vueuse/nuxt": "^10.4.1",
    "consola": "^3.2.3",
    "eslint": "^8.48.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "next-auth": "4.22.5",
    "nuxt": "^3.7.0",
    "pinia": "^2.1.6",
    "typescript": "^5.2.2",
